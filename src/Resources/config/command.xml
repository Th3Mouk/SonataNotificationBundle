<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults autowire="true" autoconfigure="true"/>
        <service id="Sonata\NotificationBundle\Command\ConsumerHandlerCommand" public="true"/>
        <service id="Sonata\NotificationBundle\Command\CreateAndPublishCommand" public="true"/>
        <service id="Sonata\NotificationBundle\Command\ListHandlerCommand" public="true"/>
        <service id="Sonata\NotificationBundle\Command\ListQueuesCommand" public="true"/>
